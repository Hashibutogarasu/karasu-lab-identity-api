{
  "name": "karasu-lab-identity-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk",
    "test:e2e": "vitest run --config ./vitest.config.ts",
    "lint": "eslint src test",
    "lint:fix": "eslint src test --fix",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,ts,json}'",
    "deploy": "sh scripts/deploy.sh",
    "setup": "sh scripts/setup.sh",
    "prisma:generate": "npx @better-auth/cli@latest generate && npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:push": "npx prisma db push",
    "prisma:pull": "npx prisma db pull"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.999.0",
    "@aws-sdk/s3-request-presigner": "^3.999.0",
    "@better-auth/passkey": "^1.4.19",
    "@hashibutogarasu/common": "github:Hashibutogarasu/karasu-lab-api-common",
    "@nestjs/common": "^11.1.14",
    "@nestjs/core": "^11.1.14",
    "@nestjs/platform-express": "^11.1.14",
    "@nestjs/swagger": "^11.2.6",
    "@prisma/adapter-pg": "^7.4.1",
    "@prisma/client": "^7.4.1",
    "@prisma/config": "^7.4.1",
    "@prisma/extension-accelerate": "^2.0.2",
    "@simplewebauthn/server": "^13.2.3",
    "@thallesp/nestjs-better-auth": "^2.4.0",
    "better-auth": "^1.4.19",
    "better-auth-firebase-auth": "^2.0.1",
    "cross-env": "^10.1.0",
    "cuid": "^3.0.0",
    "dotenv": "^17.3.1",
    "firebase": "^12.10.0",
    "firebase-admin": "^13.7.0",
    "i18next": "^25.8.13",
    "jest-mock-extended": "^4.0.0",
    "kysely": "^0.28.11",
    "pg": "^8.18.0",
    "prisma": "^7.4.1",
    "reflect-metadata": "^0.2.2",
    "resend": "^6.9.2",
    "rxjs": "^7.8.2",
    "vitest-mock-extended": "^3.1.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.4",
    "@eslint/js": "^9.39.3",
    "@flydotio/dockerfile": "^0.7.10",
    "@nestjs/cli": "^11.0.16",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.14",
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^5.0.6",
    "@types/node": "^22.19.11",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.56.1",
    "@typescript-eslint/parser": "^8.56.1",
    "better-sqlite3": "^12.6.2",
    "eslint": "^10.0.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^3.3.0",
    "eslint-plugin-unused-imports": "^4.4.1",
    "globals": "^16.5.0",
    "nest-prettier": "^0.0.4",
    "prettier": "^3.8.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.2.2",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.1",
    "unplugin-swc": "^1.5.9",
    "vitest": "^4.0.18"
  },
  "eslintConfig": {
    "extends": [
      "plugin:prettier/recommended"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "sourceRoot": "src",
          "printWidth": 100,
          "tabWidth": 2,
          "useTabs": true,
          "semi": true,
          "singleQuote": true,
          "quoteProps": "preserve",
          "trailComma": "es5",
          "bracketSpacing": true,
          "arrowParens": "always",
          "requirePragma": true,
          "insertPragma": true
        }
      ]
    }
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@firebase/util",
      "@hashibutogarasu/common",
      "@nestjs/core",
      "@prisma/client",
      "@prisma/engines",
      "@scarf/scarf",
      "@swc/core",
      "better-sqlite3",
      "esbuild",
      "prisma",
      "protobufjs",
      "unrs-resolver"
    ]
  }
}
